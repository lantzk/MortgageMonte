# Copy this file to .env.local and adjust the values as needed

# Starting Conditions - These values initialize the calculator's starting state
NEXT_PUBLIC_INCOME_SALARY=100000        # Starting household income
NEXT_PUBLIC_HOUSE_VALUE=400000          # Starting house value
NEXT_PUBLIC_HOUSE_LOAN=320000           # Starting loan amount

# Additional House-related constants
NEXT_PUBLIC_HOUSE_DOWN_PAYMENT=80000    # Down payment amount
NEXT_PUBLIC_HOUSE_PMI_MONTHLY=100       # Monthly PMI payment
NEXT_PUBLIC_HOUSE_PAYMENT=2000          # Monthly mortgage payment
NEXT_PUBLIC_HOUSE_RATE=6.5              # Mortgage interest rate (%)
NEXT_PUBLIC_HOUSE_APPRECIATION=3        # Annual house appreciation rate (%)
NEXT_PUBLIC_HOUSE_CLOSING_COSTS=5000    # Closing costs

# Additional Income-related constants
NEXT_PUBLIC_INCOME_TAX_RATE=5           # State tax rate (%)
NEXT_PUBLIC_INCOME_DEDUCTION=25000      # Standard deduction
NEXT_PUBLIC_INCOME_401K_LIMIT=22500     # Annual 401(k) contribution limit
NEXT_PUBLIC_INCOME_MATCH_RATE=4         # Employer 401(k) match rate (%)
NEXT_PUBLIC_INCOME_FICA_RATE=15.3       # FICA tax rate (%)

# Default calculator input values
NEXT_PUBLIC_DEFAULT_INVESTMENT_RETURN=7  # Default stock market return rate (%)
NEXT_PUBLIC_DEFAULT_EXTRA_PAYMENT=200    # Default extra monthly payment
NEXT_PUBLIC_DEFAULT_SALARY_GROWTH=3      # Default annual salary growth (%)
NEXT_PUBLIC_DEFAULT_MARKET_VOLATILITY=20 # Default market volatility (%)
NEXT_PUBLIC_DEFAULT_INFLATION_RATE=3     # Default inflation rate (%)
NEXT_PUBLIC_DEFAULT_LIQUIDITY_NEEDED=25000 # Default emergency fund target
NEXT_PUBLIC_DEFAULT_JOB_LOSS_PROB=0.05   # Annual job loss probability (5% based on historical unemployment rate)
NEXT_PUBLIC_DEFAULT_REFI_PROB=0.10       # Default annual refinance probability
NEXT_PUBLIC_DEFAULT_EMERGENCY_PROB=0.01   # Monthly emergency fund need probability (1% = ~11.4% annually)
NEXT_PUBLIC_DEFAULT_IBOND_LIMIT=2500     # Default annual I-Bond purchase limit
NEXT_PUBLIC_DEFAULT_MEGA_BACKDOOR=2000   # Default mega backdoor Roth amount
NEXT_PUBLIC_DEFAULT_STRESS_TEST=0.25     # Default stress test factor
NEXT_PUBLIC_DEFAULT_IBOND_BASE_RATE=1.0  # Default I-Bond base rate (%)

# Capital gains and tax brackets
NEXT_PUBLIC_CAPITAL_GAIN_RATE=15        # Long-term capital gains rate (%)
NEXT_PUBLIC_CAPITAL_DIVIDEND_RATE=20    # Dividend tax rate (%)
NEXT_PUBLIC_CAPITAL_QUALIFIED_RATE=20   # Qualified dividend rate (%)
NEXT_PUBLIC_CAPITAL_BRACKET_LOW=80000   # Lower capital gains bracket
NEXT_PUBLIC_CAPITAL_BRACKET_HIGH=500000 # Upper capital gains bracket 